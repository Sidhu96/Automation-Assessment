package com.qa.TaskOne;

import org.openqa.selenium.WebElement;


public class CreateUser {
		
	@FindBy(linkText = "Manage Jenkins")
		private WebElement manage; 
	
	public WebElement clickManage() {
		return manage; 
	}
	
	@FindBy(xpath ="xpath=(.//*[normalize-space(text()) and normalize-space(.)='Install as Windows Service'])[1]/following::dt[1]");
		private WebElement manageUser; 
		
}
